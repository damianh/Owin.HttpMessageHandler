        <releaseNotes>Fix for redirect logic: only for 301 301 304 and 307</releaseNotes>
        <projectUrl>https://github.com/damianh/OwinHttpMessageHandler</projectUrl>
        <tags>owin httpclient httpmessagehandler embedded</tags>
        <frameworkAssemblies>
            <frameworkAssembly assemblyName="System.Net.Http" targetFramework="net45"/>
        </frameworkAssemblies>
    </metadata>
    <files>
        <file src="bin\Release\OwinHttpMessageHandler.dll" target="lib\net45" />
        <file src="bin\Release\OwinHttpMessageHandler.pdb" target="lib\net45" />
        <file src="bin\Release\OwinHttpMessageHandler.xml" target="lib\net45" />
    </files>
</package>
